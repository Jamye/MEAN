<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h4>Mongoose Index Dash</h4>
    <% for (i in dis_mong) { %>
      <p>Name: <a href="/<%= dis_mong[i]._id %>"><%= dis_mong[i].name %></a>
        <a href="/<%= dis_mong[i]._id %>/edit/">Edit Mongoose</a>
        <form action="/<%= dis_mong[i]._id %>/delete/" method='post'>
          <input type="submit" value="Delete">
        </form>
      </p>
    <% } %>
  </body>
</html>
